    <string name="pattern_timing_disappear_animation_start_translation">The multiplier to move the dots up during the animation. Default: 1.2x</string>
    <string name="pattern_timing_disappear_animation_delay_scale_title">Disappear animation delay scale</string>
    <string name="pattern_timing_disappear_animation_delay_scale">The delay it takes per row to disappear after each other. Default: 0.8x</string>
    <string name="pattern_timing_disappear_animation_interpolator_title">Disappear animation interpolator</string>
