    <string name="pattern_timing_appear_animation_start_translation_title">Appear animation start translation</string>
    <string name="pattern_timing_appear_animation_start_translation">The multiplier to shift the dots down before the animation starts. Default: 1.5x</string>
    <string name="pattern_timing_appear_animation_delay_scale_title">Appear animation delay scale</string>
    <string name="pattern_timing_appear_animation_delay_scale">The delay it takes per dot to appear after each other. Default: 2.0x</string>
